<Window x:Class="TP3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TP3"
        mc:Ignorable="d"
        Title="MainWindow" Height="490.8" Width="815.6">
    <StackPanel Margin="0,0,3.6,2.6">
        <ComboBox x:Name="ComboBox1" ItemsSource="{Binding cbItems}" SelectedItem="{Binding SelectedItem}"  SelectionChanged="ComboBox1_SelectionChanged"/>
        <Label x:Name="Label1" Content="Label" />
    </StackPanel>
</Window>
