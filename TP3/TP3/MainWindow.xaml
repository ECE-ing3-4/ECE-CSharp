<Window x:Class="TP3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TP3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <ComboBox x:Name="ComboBox1" ItemsSource="{Binding cbItems}" SelectedItem="{Binding Path=SelectedItem}"   HorizontalAlignment="Left" Margin="82,76,0,0" VerticalAlignment="Top" Width="120" Grid.ColumnSpan="2" SelectionChanged="ComboBox1_SelectionChanged"/>
        <Label x:Name="Label1" Grid.ColumnSpan="2" Content="Label" HorizontalAlignment="Left" Margin="514,143,0,0" VerticalAlignment="Top"/>
    </StackPanel>
</Window>
